    "morgan": "^1.8.2",
    "mysql": "^2.13.0",
    "nexmo": "^2.0.2",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.0.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.11.0",
    "open": "0.0.5",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "pryjs": "^1.0.3",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
