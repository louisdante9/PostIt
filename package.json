{
  "name": "postit",
  "author": "louisdante9",
  "version": "1.0.0",
  "description": "PostIt is a simple application that allows friends and colleagues create groups for notifications. This way one person can post notifications to everyone by sending a message once. The application allows people create accounts, create groups and add registered users to the groups, and then send messages out to these groups whenever they want.",
  "main": "app.js",
  "engines": {
    "node": "6.9.4"
  },
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node server/startMessage.js",
    "start:dev": "npm-run-all --parallel test:watch open:server",
    "start": "npm run open:server ",
    "lint": "node_modules/.bin/esw webpack.config.* client server",
    "lint:watch": "npm run lint -- --watch",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "test": "mocha --reporter progress server/testSetup.js \"client/__tests__/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "tests": "jest",
    "tests:watch": "npm test -- --watch",
    "testserver": "cross-env NODE_ENV=test nyc --reporter=text --reporter=html --reporter=lcov mocha --compilers js:babel-core/register ./server/tests/*.test.js --timeout 150000",
    "open:server": "nodemon --watch server --exec babel-node -- ./server/app.js",
    "servertest": "npm-run-all --sequential migrate:test:down migrate:test testserver coverage-report",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:migrate",
    "migrate:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "migrate:test:down": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "jest": {
    "roots": [
      "<rootDir>/client/__tests__"
    ],
    "testURL": "http:/test.com",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/test-env.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "coveralls": "^2.13.1",
    "cross-env": "1.0.7",
    "crypto": "^1.0.1",
    "css-loader": "0.23.1",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "enzyme": "^3.1.0",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.13.4",
    "faker": "^4.1.0",
    "generate-password": "^1.3.0",
    "jquery": "2.2.3",
    "js-logger": "^1.3.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
