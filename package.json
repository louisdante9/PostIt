    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "^5.13.1",
    "file-loader": "0.8.5",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-sass": "^3.1.0",
    "jest": "^20.0.4",
